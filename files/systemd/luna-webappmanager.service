[Unit]
Description=The Luna Next WebApp manager
After=luna-next.service

[Service]
Type=simple
ExecStart=/usr/sbin/LunaWebAppManager --verbose --allow-file-access-from-files --disable-namespace-sandbox --disable-seccomp-filter-sandbox --disable-es3-gl-context
Restart=always

[Install]
WantedBy=multi-user.target
